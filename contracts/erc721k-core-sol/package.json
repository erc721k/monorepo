{
  "name": "@erc721k/core-sol",
  "version": "0.0.0-beta.0.0.1",
  "description": "",
  "main": "index.js",
  "license": "MIT",
  "files": [
    "LICENSE",
    "abis/**",
    "artifacts/**",
    "contracts/**",
    "deployments/**",
    "deploy/**"
  ],
  "scripts": {
    "cleanup": "rm -rf cache/ artifacts/",
    "clean": "rm -rf node_modules/",
    "compile": "hardhat --show-stack-traces --max-memory 8192 compile",
    "coverage": "HIDE_DEPLOY_LOG=true OPTIMIZER_DISABLED=true hardhat coverage",
    "coverage:file": "HIDE_DEPLOY_LOG=true OPTIMIZER_DISABLED=true hardhat coverage --testfiles",
    "deploy": "hardhat deploy --write true --network localhost",
    "deploy:wolf": "DEPLOY=wolf hardhat deploy --write true",
    "deploy:nounders": "DEPLOY=nounders hardhat deploy --write true",
    "deploy:mainnet": "DEPLOY=mainnet hardhat deploy --write true --network mainnet",
    "deploy:ethereum:mainnet": "hardhat deploy --write true --network mainnet",
    "deploy:ethereum:testnet": "hardhat deploy --write true --network ethereumTestnet",
    "deploy:optimism:mainnet": "hardhat deploy --write true --network optimism",
    "deploy:optimism:testnet": "hardhat deploy --write true --network optimismTestnet",
    "deploy:polygon:mainnet": "hardhat deploy --write true --network polygon",
    "deploy:polygon:testnet": "hardhat deploy --write true --network polygonTestnet",
    "deploy:fork": "hardhat deploy --write true --network hardhat",
    "verify": "hardhat etherscan-verify --solc-input --network",
    "etherscan-verify-polygon": "hardhat --config hardhat.config.polygon.js run ./scripts/verify.js --network",
    "format": "prettier --config .prettierrc --write \"**/*.{ts,js}\" \"contracts/**/*.sol\"",
    "hint": "solhint \"contracts/**/*.sol\"",
    "blockchain": "hardhat node",
    "fork:setup": "hardhat fork:setup",
    "remove-logs": "hardhat remove-logs",
    "test": "yarn cleanup && HIDE_DEPLOY_LOG=true OPTIMIZER_DISABLED=true hardhat test",
    "typechain": "typechain --target=ethers-v5 \"./abis/*.json\" --out-dir \"./types\"",
    "prepack": "yarn compile",
    "prepare": "hardhat --show-stack-traces --max-memory 8192 compile"
  },
  "devDependencies": {
    "@ethersproject/abi": "5.6.1",
    "@erc721k/periphery-sol": "0.0.0-beta.0",
    "@erc721k/streams-sol": "0.0.0-beta.0",
    "@nomicfoundation/hardhat-toolbox": "^1.0.2",
    "@nomiclabs/hardhat-ethers": "2.0.4",
    "@nomiclabs/hardhat-etherscan": "3.0.0",
    "@nomiclabs/hardhat-waffle": "2.0.2",
    "@openzeppelin/hardhat-upgrades": "1.14.0",
    "@turbo-eth/solbase-sol": "0.0.1",
    "@typechain/ethers-v5": "9.0.0",
    "@typechain/hardhat": "4.0.0",
    "@types/chai": "4.3.0",
    "@types/debug": "4.1.7",
    "@types/mocha": "9.1.0",
    "@types/node": "^18.0.0",
    "base64-sol": "1.1.0",
    "chai": "4.3.6",
    "chalk": "5.0.0",
    "debug": "4.3.3",
    "dotenv": "^16.0.1",
    "ethereum-waffle": "3.4.0",
    "ethers": "5.6.0",
    "hardhat": "2.10.1",
    "hardhat-abi-exporter": "2.8.0",
    "hardhat-dependency-compiler": "1.1.2",
    "hardhat-deploy": "0.10.5",
    "hardhat-deploy-ethers": "0.3.0-beta.13",
    "hardhat-ethers": "1.0.1",
    "hardhat-gas-reporter": "1.0.7",
    "hardhat-log-remover": "2.0.2",
    "husky": "^8.0.1",
    "node-watch": "0.7.3",
    "prettier": "^2.6.2",
    "prettier-plugin-solidity": "1.0.0-beta.19",
    "solc": "0.8.15",
    "solhint": "3.3.6",
    "solidity-coverage": "0.7.18",
    "solidity-docgen": "0.5.16",
    "terminal-image": "^2.0.0",
    "ts-generator": "0.1.1",
    "ts-node": "^10.8.1",
    "typechain": "7.0.0",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "@openzeppelin/contracts": "^4.6.0"
  }
}
